<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape>
        <corners android:radius="10dp"/>
        <stroke android:width="0.5dip" android:color="@color/line_color_d1d1d1" />
        <gradient android:startColor="#f3741c" android:endColor="#da5c04" android:angle="-90" ></gradient>
    </shape>
    </item>
    <item  android:state_pressed="true">
    <shape>
        <corners android:radius="5dp"/>
        <stroke android:width="0.5dip" android:color="@color/line_color_d1d1d1" />
        <gradient android:startColor="#e35412" android:endColor="#ca4c04" android:angle="-90" ></gradient>
    </shape>
</item>
</selector>
